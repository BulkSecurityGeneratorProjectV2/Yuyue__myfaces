<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>

    <tlibversion>0.1</tlibversion>
    <jspversion>1.2</jspversion>
    <shortname>MyFaces Basic Tag Library</shortname>
    <uri>http://myfaces.sourceforge.net/tld/myfaces_0_1.tld</uri>
    <info>
        Tag library for the MyFaces basic implementation.
    </info>

    <tag>
        <name>use_faces</name>
        <tagclass>net.sourceforge.myfaces.taglib.UseFacesTag</tagclass>
        <bodycontent>JSP</bodycontent>
    </tag>

    <tag>
        <name>output_text</name>
        <tagclass>net.sourceforge.myfaces.taglib.OutputTextTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <name>modelReference</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <name>text</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>converter</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>form</name>
        <tagclass>net.sourceforge.myfaces.taglib.FormTag</tagclass>
