<faces-config xmlns="http://java.sun.com/JSF/Configuration">


  <!-- MyFaces extended standard components (in alphabetical order) -->

  <component>
    <component-type>net.sourceforge.myfaces.HtmlCommandButton</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlCommandButton</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlCommandLink</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlCommandLink</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlDataTable</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlDataTable</component-class>
  </component>
  <lifecycle>
    <phase-listener>net.sourceforge.myfaces.component.html.ext.HtmlDataTablePhaseListener</phase-listener>
  </lifecycle>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlInputText</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlInputText</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlMessage</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlMessage</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlMessages</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlMessages</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlOutputLabel</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlOutputLabel</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlOutputText</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlOutputText</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlPanelGroup</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlPanelGroup</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlSelectOneMenu</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlSelectOneMenu</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlSelectOneRadio</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlSelectOneRadio</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlSelectManyCheckbox</component-type>
    <component-class>net.sourceforge.myfaces.component.html.ext.HtmlSelectManyCheckbox</component-class>
  </component>

  <!-- MyFaces custom components (in alphabetical order) -->

  <component>
    <component-type>net.sourceforge.myfaces.HtmlCheckbox</component-type>
    <component-class>net.sourceforge.myfaces.custom.checkbox.HtmlCheckbox</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlCommandNavigation</component-type>
    <component-class>net.sourceforge.myfaces.custom.navigation.HtmlCommandNavigation</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlCommandSortHeader</component-type>
    <component-class>net.sourceforge.myfaces.custom.sortheader.HtmlCommandSortHeader</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlInputCalendar</component-type>
    <component-class>net.sourceforge.myfaces.custom.calendar.HtmlInputCalendar</component-class>
  </component>
  
  <component>
    <component-type>net.sourceforge.myfaces.HtmlInputDate</component-type>
    <component-class>net.sourceforge.myfaces.custom.date.HtmlInputDate</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlInputFileUpload</component-type>
    <component-class>net.sourceforge.myfaces.custom.fileupload.HtmlInputFileUpload</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlDataList</component-type>
    <component-class>net.sourceforge.myfaces.custom.datalist.HtmlDataList</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlDataScroller</component-type>
    <component-class>net.sourceforge.myfaces.custom.datascroller.HtmlDataScroller</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlPanelLayout</component-type>
    <component-class>net.sourceforge.myfaces.custom.layout.HtmlPanelLayout</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlPanelNavigation</component-type>
    <component-class>net.sourceforge.myfaces.custom.navigation.HtmlPanelNavigation</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlPanelTab</component-type>
    <component-class>net.sourceforge.myfaces.custom.tabbedpane.HtmlPanelTab</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlPanelTabbedPane</component-type>
    <component-class>net.sourceforge.myfaces.custom.tabbedpane.HtmlPanelTabbedPane</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.HtmlRadio</component-type>
    <component-class>net.sourceforge.myfaces.custom.radio.HtmlRadio</component-class>
  </component>

  <component>
      <component-type>net.sourceforge.myfaces.HtmlTree</component-type>
      <component-class>net.sourceforge.myfaces.custom.tree.HtmlTree</component-class>
  </component>

  <component>
      <component-type>net.sourceforge.myfaces.HtmlTreeNode</component-type>
      <component-class>net.sourceforge.myfaces.custom.tree.HtmlTreeNode</component-class>
  </component>

  <component>
        <component-type>net.sourceforge.myfaces.RssTicker</component-type>
      <component-class>net.sourceforge.myfaces.custom.rssticker.HtmlRssTicker</component-class>
  </component>

  <component>
      <component-type>net.sourceforge.myfaces.HtmlTreeImageCommandLink</component-type>
      <component-class>net.sourceforge.myfaces.custom.tree.HtmlTreeImageCommandLink</component-class>
  </component>

  <component>
      <component-type>net.sourceforge.myfaces.JSCookMenu</component-type>
      <component-class>net.sourceforge.myfaces.custom.navmenu.jscookmenu.HtmlCommandJSCookMenu</component-class>
  </component>

    <component>
        <component-type>net.sourceforge.myfaces.NavigationMenuItem</component-type>
        <component-class>net.sourceforge.myfaces.custom.navmenu.UINavigationMenuItem</component-class>
    </component>

  <component>
      <component-type>net.sourceforge.myfaces.RssTicker</component-type>
      <component-class>net.sourceforge.myfaces.custom.rssticker.HtmlRssTicker</component-class>
  </component>

  <component>
    <component-type>net.sourceforge.myfaces.SaveState</component-type>
    <component-class>net.sourceforge.myfaces.custom.savestate.UISaveState</component-class>
  </component>

    <!-- additional "by type" converters -->

    <converter>
        <converter-for-class>java.math.BigDecimal</converter-for-class>
        <converter-class>javax.faces.convert.BigDecimalConverter</converter-class>
    </converter>

    <converter>
        <converter-for-class>java.math.BigInteger</converter-for-class>
        <converter-class>javax.faces.convert.BigIntegerConverter</converter-class>
    </converter>

    <converter>
        <converter-for-class>java.util.Date</converter-for-class>
        <converter-class>javax.faces.convert.DateTimeConverter</converter-class>
    </converter>


    <!-- non standard converters -->

    <converter>
        <converter-for-class>[Ljava.lang.String;</converter-for-class>
        <converter-class>net.sourceforge.myfaces.convert.StringArrayConverter</converter-class>
    </converter>

    <converter>
        <converter-for-class>net.sourceforge.myfaces.custom.fileupload.UploadedFile</converter-for-class>
        <converter-class>net.sourceforge.myfaces.custom.fileupload.UploadedFileConverter</converter-class>
    </converter>

    

    <!-- MyFaces Renderkit Extensions -->

    <render-kit>
        <render-kit-id>HTML_BASIC</render-kit-id>

        <!-- extended standard renderers -->

        <renderer>
            <component-family>javax.faces.Command</component-family>
            <renderer-type>net.sourceforge.myfaces.Button</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlButtonRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Command</component-family>
            <renderer-type>net.sourceforge.myfaces.Link</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlLinkRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Input</component-family>
            <renderer-type>net.sourceforge.myfaces.Text</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlTextRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Message</component-family>
            <renderer-type>net.sourceforge.myfaces.Message</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlMessageRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Messages</component-family>
            <renderer-type>net.sourceforge.myfaces.Messages</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlMessagesRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Output</component-family>
            <renderer-type>net.sourceforge.myfaces.Text</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlTextRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.SelectMany</component-family>
            <renderer-type>net.sourceforge.myfaces.Checkbox</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlCheckboxRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.SelectOne</component-family>
            <renderer-type>net.sourceforge.myfaces.Menu</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlMenuRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.SelectOne</component-family>
            <renderer-type>net.sourceforge.myfaces.Radio</renderer-type>
            <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlRadioRenderer</renderer-class>
        </renderer>


        <!-- custom component renderers -->

      <renderer>
          <component-family>net.sourceforge.myfaces.Checkbox</component-family>
          <renderer-type>net.sourceforge.myfaces.Checkbox</renderer-type>
          <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlCheckboxRenderer</renderer-class>
      </renderer>

        <renderer>
            <component-family>javax.faces.Panel</component-family>
            <renderer-type>net.sourceforge.myfaces.DataScroller</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.datascroller.HtmlDataScrollerRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Input</component-family>
            <renderer-type>net.sourceforge.myfaces.Calendar</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.calendar.HtmlCalendarRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Input</component-family>
            <renderer-type>net.sourceforge.myfaces.Date</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.date.HtmlDateRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Input</component-family>
            <renderer-type>net.sourceforge.myfaces.FileUpload</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.fileupload.HtmlFileUploadRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Panel</component-family>
            <renderer-type>net.sourceforge.myfaces.Layout</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.layout.HtmlLayoutRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Panel</component-family>
            <renderer-type>net.sourceforge.myfaces.Navigation</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.navigation.HtmlNavigationRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Command</component-family>
            <renderer-type>net.sourceforge.myfaces.Navigation</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.navigation.HtmlNavigationRenderer</renderer-class>
        </renderer>
        
        <renderer>
            <component-family>javax.faces.Command</component-family>
            <renderer-type>net.sourceforge.myfaces.JSCookMenu</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.navmenu.jscookmenu.HtmlJSCookMenuRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Command</component-family>
            <renderer-type>net.sourceforge.myfaces.SortHeader</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.sortheader.HtmlSortHeaderRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>javax.faces.Panel</component-family>
            <renderer-type>net.sourceforge.myfaces.TabbedPane</renderer-type>
            <renderer-class>net.sourceforge.myfaces.custom.tabbedpane.HtmlTabbedPaneRenderer</renderer-class>
        </renderer>

      <renderer>
          <component-family>javax.faces.Data</component-family>
          <renderer-type>net.sourceforge.myfaces.List</renderer-type>
          <renderer-class>net.sourceforge.myfaces.custom.datalist.HtmlListRenderer</renderer-class>
      </renderer>

      <renderer>
          <component-family>javax.faces.Output</component-family>
          <renderer-type>net.sourceforge.myfaces.Ticker</renderer-type>
          <renderer-class>net.sourceforge.myfaces.custom.rssticker.HtmlRssTickerRenderer</renderer-class>
      </renderer>

      <renderer>
          <component-family>net.sourceforge.myfaces.Radio</component-family>
          <renderer-type>net.sourceforge.myfaces.Radio</renderer-type>
          <renderer-class>net.sourceforge.myfaces.renderkit.html.ext.HtmlRadioRenderer</renderer-class>
      </renderer>


       <renderer>
          <component-family>net.sourceforge.myfaces.HtmlTree</component-family>
          <renderer-type>net.sourceforge.myfaces.HtmlTree</renderer-type>
          <renderer-class>net.sourceforge.myfaces.custom.tree.renderkit.html.HtmlTreeRenderer</renderer-class>
       </renderer>

       <renderer>
          <component-family>net.sourceforge.myfaces.HtmlTree</component-family>
          <renderer-type>net.sourceforge.myfaces.HtmlTreeImageCommandLink</renderer-type>
          <renderer-class>net.sourceforge.myfaces.custom.tree.renderkit.html.HtmlTreeImageCommandLinkRenderer</renderer-class>
       </renderer>

       <renderer>
          <component-family>javax.faces.Command</component-family>
          <renderer-type>net.sourceforge.myfaces.HtmlTreeNode</renderer-type>
          <renderer-class>net.sourceforge.myfaces.custom.tree.renderkit.html.HtmlTreeNodeRenderer</renderer-class>
       </renderer>
        

    </render-kit>
    
    <!--custom validators based upon jakarta commons validator-->
	<validator>
		<validator-id>net.sourceforge.myfaces.validator.CreditCard</validator-id>
		<validator-class>net.sourceforge.myfaces.custom.creditcardvalidator.CreditCardValidator</validator-class>
	</validator>

	<validator>
		<validator-id>net.sourceforge.myfaces.validator.Email</validator-id>
		<validator-class>net.sourceforge.myfaces.custom.emailvalidator.EmailValidator</validator-class>
	</validator>

	<validator>
		<validator-id>net.sourceforge.myfaces.validator.Equal</validator-id>
		<validator-class>net.sourceforge.myfaces.custom.equalvalidator.EqualValidator</validator-class>
	</validator>

	<validator>
		<validator-id>net.sourceforge.myfaces.validator.RegExpr</validator-id>
		<validator-class>net.sourceforge.myfaces.custom.regexprvalidator.RegExprValidator</validator-class>
	</validator>

</faces-config>