<faces-config xmlns="http://java.sun.com/JSF/Configuration">

  <factory>
    <application-factory>net.sourceforge.myfaces.application.ApplicationFactoryImpl</application-factory>
    <faces-context-factory>net.sourceforge.myfaces.context.FacesContextFactoryImpl</faces-context-factory>
    <lifecycle-factory>net.sourceforge.myfaces.lifecycle.LifecycleFactoryImpl</lifecycle-factory>
    <render-kit-factory>net.sourceforge.myfaces.renderkit.RenderKitFactoryImpl</render-kit-factory>
  </factory>

  <!-- Initial application element with partial values -->
  <application>
    <action-listener>net.sourceforge.myfaces.application.ActionListenerImpl</action-listener>
    <navigation-handler>net.sourceforge.myfaces.application.NavigationHandlerImpl</navigation-handler>
    <variable-resolver>net.sourceforge.myfaces.el.VariableResolverImpl</variable-resolver>
    <property-resolver>net.sourceforge.myfaces.el.PropertyResolverImpl</property-resolver>
    <!--message-bundle></message-bundle-->
    <locale-config>
        <default-locale>en</default-locale>
        <supported-locale>en</supported-locale>
        <supported-locale>de</supported-locale>
    </locale-config>
  </application>


